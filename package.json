{
  "name": "react-query-ofetch",
  "version": "0.0.0-dev",
  "description": "React Query bindings for ofetch",
  "author": "Josh Willox <joshwillox@gmail.com> (https://github.com/jcwillox/)",
  "license": "MIT",
  "repository": "jcwillox/react-query-ofetch",
  "keywords": [
    "react-query",
    "ofetch",
    "use-fetch"
  ],
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify",
    "typecheck": "tsc",
    "lint": "eslint . --cache --max-warnings=0 --ext js,cjs,mjs,jsx,ts,tsx",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check ."
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "is-plain-obj": "^4.1.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "5.52.2",
    "@tsconfig/node-lts": "20.1.3",
    "@types/node": "22.8.2",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "ofetch": "1.3.4",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tsup": "8.2.4",
    "tsx": "4.19.0",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.17.0",
    "ofetch": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./options": {
      "import": {
        "types": "./dist/options.d.ts",
        "default": "./dist/options.js"
      },
      "require": {
        "types": "./dist/options.d.cts",
        "default": "./dist/options.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      },
      "require": {
        "types": "./dist/utils.d.cts",
        "default": "./dist/utils.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": "^18 || ^20 || ^22.0.0"
  }
}
